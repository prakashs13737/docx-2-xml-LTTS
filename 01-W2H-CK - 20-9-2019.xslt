              </xsl:choose>
              <!--</xsl:if>-->
            </xsl:for-each>
          </xsl:if>
        </xsl:when>              
        <xsl:otherwise>    
          <xsl:call-template name="margin"/> 
        </xsl:otherwise>
      </xsl:choose>
      <!--</xsl:if>-->
    </xsl:for-each>
