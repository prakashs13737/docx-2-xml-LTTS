        <xsl:element name="sup">
          <xsl:apply-templates/>
        </xsl:element>
      </xsl:when>
      <xsl:when test="w:rPr/w:vertAlign[@w:val='subscript']">
        <xsl:element name="sub">
          <xsl:apply-templates/>
        </xsl:element>
      </xsl:when>
      <xsl:when test="w:rPr/w:position[number(@w:val)&lt;-2]">
        <xsl:element name="sub">
          <xsl:apply-templates/>
        </xsl:element>
      </xsl:when>
      <xsl:when test="w:rPr/w:strike">
