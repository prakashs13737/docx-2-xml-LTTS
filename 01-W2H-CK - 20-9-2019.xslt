          <xsl:apply-templates/>
        </xsl:element>
      </xsl:when>
      <xsl:when test="w:rPr/w:u[@w:val='double']">
        <xsl:element name="underdoubleline">
          <xsl:apply-templates/>
        </xsl:element>
      </xsl:when>
      <xsl:when test="w:rPr/w:smallCaps[not(@w:val = '0' or @w:val = 'false')]">
        <xsl:element name="small">
          <xsl:apply-templates/>
        </xsl:element>
