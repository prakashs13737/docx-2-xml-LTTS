                      </xsl:when>
                      <xsl:when test="$baseline = 'superscript'">
                        <xsl:text>vertical-align:super;</xsl:text>
                      </xsl:when>
                      <xsl:otherwise>
                        <xsl:text>vertical-align:baseline;</xsl:text>
                      </xsl:otherwise>
                    </xsl:choose>
                    <xsl:text>&#x000A;</xsl:text>
                  </xsl:if>  
            <xsl:if test="./w:basedOn">
              <xsl:call-template name="loop"/>
            </xsl:if>
            <!--</xsl:if>-->
     <!--</xsl:if>-->
    </xsl:for-each>
  </xsl:template>
  
