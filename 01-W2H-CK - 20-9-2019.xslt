  <xsl:template name="cell.morerows">
    <xsl:variable name="span" select="w:tcPr/w:gridSpan/@w:val"/>
    <xsl:variable name="colstart">
      <xsl:call-template name="colcount">
        <xsl:with-param name="count" select="1"/>
