          <xsl:text>pt;</xsl:text>
        </xsl:when>
        <xsl:otherwise>
          <xsl:call-template name="loop"/>
        </xsl:otherwise>
      </xsl:choose>
      <!--<xsl:if test="w:rPr/w:sz">
                    <xsl:text>font-size:</xsl:text>
                    <xsl:variable name="fontSize" select="number(w:rPr/w:sz/@w:val) div 2"/>
                    <xsl:value-of select="$fontSize"/>
                    <xsl:text>pt;</xsl:text>           
