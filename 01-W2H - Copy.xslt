                  <xsl:if test="child::w:tcPr/w:tcBorders/w:right[@w:val = 'double']">
                    <xsl:text>border-right:2px double black;</xsl:text>
                  </xsl:if>
                </xsl:attribute>
              </xsl:otherwise>
            </xsl:choose>
          </xsl:when>
        </xsl:choose>
