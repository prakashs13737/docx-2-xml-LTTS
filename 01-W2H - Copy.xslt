                        <!-- ***********w:b********** -->
                        <xsl:when
                          test="w:rPr/w:b | w:rPr/w:i | w:rPr/w:rFonts | w:rPr/w:sz | w:rPr/w:color | w:rPr/w:position | w:rPr/w:smallCaps | w:rPr/w:u | w:rPr/w:strike | w:rPr/w:caps | w:rPr/w:dstrike | w:rPr/w:vertAlign">

