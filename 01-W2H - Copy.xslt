                          <xsl:if test="w:rPr/w:vertAlign[@w:val = 'superscript']">
                            <xsl:text disable-output-escaping="yes">&lt;sup&gt;</xsl:text>
                          </xsl:if>
                          <xsl:if test="w:rPr/w:vertAlign[@w:val = 'subscript']">
