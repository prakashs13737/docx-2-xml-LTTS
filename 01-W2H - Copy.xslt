                  <xsl:text>pt;</xsl:text>

                </xsl:when>

                <xsl:when test="./w:basedOn">
                  <xsl:call-template name="margin"/>
                </xsl:when>
              </xsl:choose>
              <!--</xsl:if>-->
            </xsl:for-each>
          </xsl:if>
        </xsl:when>
        <xsl:otherwise>
          <xsl:call-template name="margin"/>
        </xsl:otherwise>
      </xsl:choose>
      <!--</xsl:if>-->
    </xsl:for-each>
