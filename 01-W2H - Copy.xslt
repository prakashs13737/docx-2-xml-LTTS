                            <xsl:text disable-output-escaping="yes">&lt;/s&gt;</xsl:text>
                          </xsl:if>
                          <xsl:if test="w:rPr/w:u">
                            <xsl:choose>
                              <xsl:when test="w:rPr/w:u[@w:val = 'double']">
                                <xsl:text disable-output-escaping="yes">&lt;/span&gt;</xsl:text>
