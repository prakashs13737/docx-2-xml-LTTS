                          <!-- Group SPAN tag -->
                          <xsl:if
                            test="
                              w:rPr/w:rFonts | w:rPr/w:szCs | w:rPr/w:sz | w:rPr/w:color | w:rPr/w:position | w:u | w:rPr/w:smallCaps | w:rPr/w:highlight | w:rPr/w:b[@w:val = '0'] | w:rPr/w:b[@w:val = 'false']
