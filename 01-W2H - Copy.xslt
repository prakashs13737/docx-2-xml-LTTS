                              <xsl:if
                                test="not(w:rPr/w:b[@w:val = 'false' or @w:val = '0'] | w:rPr/w:i[@w:val = 'false' or @w:val = '0'] | w:rPr/w:sz | w:rPr/w:szCs | w:rPr/w:rFonts | w:rPr/w:color | w:rPr/w:highlight)">
                                <xsl:text> style="</xsl:text>
