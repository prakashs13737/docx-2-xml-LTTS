                <xsl:value-of select="$aSapce div 20"/>
                <xsl:text>pt;</xsl:text>
              </xsl:if>
            </xsl:otherwise>
          </xsl:choose>
        </xsl:when>

        <xsl:otherwise>
          <xsl:call-template name="margin"/>
        </xsl:otherwise>
      </xsl:choose>
      <xsl:choose>
        <xsl:when test="./w:pPr/w:ind">

